{
  "cSpell.words": [
    "Akismet",
    "astrojs",
    "Elysia",
    "errmsg",
    "nodejieba",
    "optim",
    "POSTHOG",
    "srcset",
    "stringbool",
    "tfidf",
    "TFIDF",
    "waline",
    "Yuline"
  ],

  "typescript.tsdk": "node_modules/typescript/lib",

  "files.associations": {
    "*.css": "tailwindcss"
  },
  "tailwindCSS.experimental.configFile": {
    "app/blog-astro/src/styles/global.css": [
      "lib/comment*/**/*",
      "app/blog-astro/**/*"
    ],
    "app/blog-next/src/app/globals.css": ["app/blog-next/**/*"]
  }
}
